{
    "name": "lit-element-boilerplate",
    "version": "1.0.0",
    "description": "LitElement Boilerplate | Typescript",
    "scripts": {
        "build": "webpack",
        "start": "npm run build && webpack-dev-server",
        "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src",
        "test": "karma start",
        "packagr": "npm run build --prod && npm pack ./dist"
    },
    "engines": {
        "node": "10.16.3",
        "npm": "6.9.0"
    },
    "dependencies": {
        "@webcomponents/webcomponentsjs": "2.2.10",
        "bootstrap": "^4.4.1",
        "lit-element": "2.2.1",
        "regenerator-runtime": "0.13.3"
    },
    "devDependencies": {
        "@babel/cli": "7.6.2",
        "@babel/core": "7.6.2",
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "@babel/plugin-proposal-decorators": "7.6.0",
        "@babel/preset-env": "7.6.2",
        "@types/jasmine": "3.4.0",
        "babel-loader": "8.0.6",
        "clean-webpack-plugin": "3.0.0",
        "css-loader": "3.2.0",
        "generate-json-webpack-plugin": "0.3.1",
        "html-webpack-plugin": "3.2.0",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine": "3.5.0",
        "karma": "4.3.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "2.0.1",
        "karma-webpack": "4.0.2",
        "node-sass": "4.14.1",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "sass-loader": "8.0.0",
        "terser-webpack-plugin": "2.0.1",
        "to-string-loader": "1.1.5",
        "ts-loader": "6.1.2",
        "tslint": "5.20.0",
        "typedoc": "0.15.0",
        "typescript": "3.6.3",
        "webpack": "4.41.0",
        "webpack-cli": "3.3.9",
        "webpack-dev-server": "3.8.1",
        "webpack-merge": "4.2.2"
    }
}
